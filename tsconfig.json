{
    "compilerOptions": {
        "outDir": "./assets/js",
        "target": "es2015",
        "module": "commonjs",
        "lib":["dom", "es2017"],
        "removeComments": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "sourceMap": true,
        "declaration": false
    },
    "include": [
        "./src/ts/*"
    ]
}
