    "ps-node": "^0.1.6",
    "request": "^2.85.0",
    "request-progress": "^3.0.0",
    "underscore": "^1.9.0",
    "uuid": "^3.2.1",
    "var": "^0.3.2",
    "yauzl": "^2.9.1"
  }
