{
  "name": "viur-control",
  "version": "0.3.0",
  "description": "The better App Engine Launcher",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --debug",
    "package": "npm-run-all package:*",
    "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=out --icon=assets/img/viur.icns --osx-sign.identity='Developer ID Application: Mausbrand' --extend-info=assets/mac/info.plist",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=out",
    "package:linux": "electron-packager . --overwrite --platform=linux --arch=x64 --out=out",
    "deb64": "dependency-installer/linux/ubuntu/fixperms.sh && electron-installer-debian --src out/viur-control-linux-x64/ --dest out/installers/ --config dependency-installer/linux/ubuntu/debian-options.json"
  },
  "keywords": [
    "development",
    "electron",
    "gcloud",
    "google",
    "app engine",
    "python",
    "typescript",
    "viur"
  ],
  "author": "Stefan KÃ¶gl <sk@mausbrand.de>",
  "homepage": "https://github.com/viur-framework/control",
  "license": "GPL-3.0",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/viur-framework/control.git"
  },
  "devDependencies": {
    "@types/electron-store": "^1.3.0",
    "@types/node": "^10.0.6",
    "electron": "^2.0.0",
    "electron-installer-debian": "*",
    "electron-installer-redhat": "*",
    "electron-installer-windows": "*",
    "electron-ipc-log": "^3.0.1",
    "electron-packager": "^10.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-jshint": "^2.0.4",
    "gulp-notify": "^3.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-strip-debug": "^1.1.0",
    "gulp-typescript": "^3.2.3",
    "gulp-uglify": "^3.0.0",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "async": "^2.6.0",
    "class-transformer": "^0.1.9",
    "electron-localshortcut": "^2.0.2",
    "electron-positioner": "^3.0.0",
    "electron-store": "^1.3.0",
    "fs-extra": "^4.0.3",
    "globby": "^7.1.1",
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "mustache": "^2.3.0",
    "node-glob": "^1.2.0",
    "npm": "^6.0.0",
    "ps-node": "^0.1.6",
    "request": "^2.85.0",
    "request-progress": "^3.0.0",
    "underscore": "^1.9.0",
    "uuid": "^3.2.1",
    "var": "^0.3.2",
    "yauzl": "^2.9.1"
  }
}
